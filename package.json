{
  "name": "cyber-bot",
  "version": "2.2.1",
  "description": "Cyber Bot is an Node.js project designed to help users improve their cybersecurity skills through scheduled Telegram messages",
  "main": "index.js",
  "scripts": {
    "cron": "./cron.sh",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .",
    "prettier": "./node_modules/.bin/prettier --write .",
    "prettier:check": "./node_modules/.bin/prettier --check .",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --bail",
    "check-coverage": "jest --coverage --coverageThreshold='{\"global\":{\"statements\":50,\"branches\":50,\"functions\":50,\"lines\":50}}'"
  },
  "keywords": [],
  "author": "kOaDT",
  "repository": "https://github.com/kOaDT/cyber-bot",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@mistralai/mistralai": "^1.14.1",
    "@supadata/js": "^1.4.0",
    "assemblyai": "^4.23.1",
    "cheerio": "^1.2.0",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "mysql2": "^3.18.1",
    "node-telegram-bot-api": "^0.67.0",
    "puppeteer": "^24.37.5",
    "request": "^2.88.2",
    "rss-parser": "^3.13.0",
    "winston": "^3.19.0",
    "youtube-dl-exec": "^3.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.4",
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security-node": "^1.1.4",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
