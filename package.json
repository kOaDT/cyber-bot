{
  "name": "cyber-bot",
  "version": "1.18.10",
  "description": "Cyber Bot is an Node.js project designed to help users improve their cybersecurity skills through scheduled Telegram messages",
  "main": "index.js",
  "scripts": {
    "cron": "./cron.sh",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .",
    "prettier": "./node_modules/.bin/prettier --write .",
    "prettier:check": "./node_modules/.bin/prettier --check .",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --bail",
    "check-coverage": "jest --coverage --coverageThreshold='{\"global\":{\"statements\":50,\"branches\":50,\"functions\":50,\"lines\":50}}'"
  },
  "keywords": [],
  "author": "kOaDT",
  "repository": "https://github.com/kOaDT/cyber-bot",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "@mistralai/mistralai": "^1.9.18",
    "assemblyai": "^4.15.0",
    "cheerio": "^1.1.2",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "mysql2": "^3.14.3",
    "node-telegram-bot-api": "^0.66.0",
    "puppeteer": "^24.17.1",
    "request": "^2.88.2",
    "rss-parser": "^3.13.0",
    "winston": "^3.17.0",
    "youtube-dl-exec": "^3.0.24",
    "youtubei.js": "^15.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@types/jest": "^30.0.0",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security-node": "^1.1.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.1.1",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
